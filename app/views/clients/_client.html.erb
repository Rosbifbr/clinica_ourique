<div id="<%= dom_id client %>">
  <p>
    <strong>Name:</strong>
    <%= client.name %>
  </p>

  <p>
    <strong>CPF:</strong>
    <%= client.cpf %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= client.phone %>
  </p>

  <p>
    <strong>Birthdate:</strong>
    <%= client.birthdate %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= client.address %>
  </p>

  <p>
    <strong>Postal code:</strong>
    <%= client.postal_code %>
  </p>

  <p>
    <strong>Neighborhood:</strong>
    <%= client.neighborhood %>
  </p>

</div>
